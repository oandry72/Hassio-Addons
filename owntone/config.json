{
  "name": "OwnTone",
  "version": "0.0.2",
  "slug": "owntone",
  "description": "OwnTone media server instance",
  "url": "https://github.com/oandry72/Hassio-Addons/tree/main/owntone",
  "startup": "services",
  "host_network": true,
  "host_dbus": true,
  "audio": true,
  "devices": ["/dev/snd"],
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "boot": "auto",
  "map": [
    "config:rw",
    "media:rw"
  ],
  "options": {
    "beer": true,
    "wine": true,
    "liquor": false,
    "name": "world",
    "year": 2017
  },
  "schema": {
    "beer": "bool",
    "wine": "bool",
    "liquor": "bool",
    "name": "str",
    "year": "int"
  }
}
